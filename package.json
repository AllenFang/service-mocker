{
  "name": "service-mocker",
  "version": "0.0.1",
  "description": "Next generation api mocker.",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "lint": "tslint -t stylish -e '**/*.d.ts' '{src, demo}/**/*.ts'",
    "clean:build": "rimraf ./build",
    "clean:compile": "rimraf ./lib",
    "clean:release": "rimraf ./dist",
    "clean": "npm-run-all clean:build clean:compile clean:release",
    "compile": "tsc -p tsconfig.prod.json",
    "release": "./scripts/release.sh",
    "start": "node ./scripts/serve.js",
    "pretest": "npm run lint",
    "test": "npm-run-all --parallel test:modern test:legacy",
    "test:full": "karma start ./scripts/karma.config.full.js",
    "test:modern": "cross-env karma --port 9876 start ./scripts/karma.config.dev.js",
    "test:legacy": "cross-env FORCE_LEGACY=true karma --port 9877 start ./scripts/karma.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idiotWu/service-mocker.git"
  },
  "keywords": [
    "mock",
    "service",
    "worker"
  ],
  "author": "Dolphin Wood <dolphin.w.e@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/idiotWu/service-mocker/issues"
  },
  "homepage": "https://github.com/idiotWu/service-mocker#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "circular-dependency-plugin": "^2.0.0",
    "cross-env": "^3.1.3",
    "es6-promise": "^4.0.5",
    "html-webpack-plugin": "^2.24.1",
    "ip": "^1.1.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-remap-coverage": "^0.1.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.1",
    "mocha": "^3.2.0",
    "mocha-loader": "^1.0.0",
    "npm-run-all": "^3.1.2",
    "rimraf": "^2.5.4",
    "source-map-support": "^0.4.8",
    "ts-loader": "^1.3.0",
    "tslint": "^4.0.2",
    "tslint-config-standard": "^2.0.0",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.1.4",
    "webpack": "^1.13.3",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^1.16.2",
    "whatwg-fetch": "^2.0.1"
  },
  "dependencies": {
    "localforage": "^1.4.3",
    "tslib": "^1.2.0",
    "url": "^0.11.0"
  }
}
