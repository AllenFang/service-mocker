{
  "name": "service-mocker",
  "version": "0.0.1",
  "description": "Next generation api mocker.",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "lint": "tslint -t stylish '{src, demo}/**/*.ts'",
    "clean:build": "rimraf ./build",
    "clean:compile": "rimraf ./lib",
    "clean:release": "rimraf ./dist",
    "precompile": "npm run clean:compile",
    "compile": "tsc -p tsconfig.prod.json",
    "prerelease": "npm run clean:release",
    "release": "node ./scripts/release",
    "preghpages": "npm run clean:build",
    "ghpages": "webpack --config ./webpack/ghpages.js --display-error-details --color",
    "start": "node ./webpack/serve.js",
    "pretest": "npm run lint",
    "test": "webpack --config ./webpack/test.js --display-error-details --color"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idiotWu/service-mocker.git"
  },
  "keywords": [
    "mock",
    "service",
    "worker"
  ],
  "author": "Dolphin Wood <dolphin.w.e@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/idiotWu/service-mocker/issues"
  },
  "homepage": "https://github.com/idiotWu/service-mocker#readme",
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "circular-dependency-plugin": "^2.0.0",
    "ip": "^1.1.4",
    "node-sass": "^3.13.0",
    "postcss-loader": "^1.1.1",
    "promise-polyfill": "^6.0.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "shelljs": "^0.7.5",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.3.0",
    "tslint": "^4.0.2",
    "tslint-config-standard": "^2.0.0",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.1.4",
    "webpack": "^1.13.3",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^1.16.2",
    "whatwg-fetch": "^2.0.1"
  },
  "dependencies": {
    "localforage": "^1.4.3",
    "tslib": "^1.2.0"
  },
  "files": [
    "src/",
    "lib/",
    "package.json",
    "typings.json",
    "README.md",
    "LICENSE"
  ]
}
