{
  "compilerOptions": {
    "target": "es5",
    "lib": [
        "dom",
        "es2017"
    ],
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "importHelpers": true,
    "declaration": true,
    "sourceMap": true
  },
  "exclude": [
    "node_modules",
    "lib",
    "dist"
  ]
}
